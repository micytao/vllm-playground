# Include static assets
recursive-include vllm_playground/static *
recursive-include vllm_playground/assets *
recursive-include vllm_playground/config *
include vllm_playground/*.html
include vllm_playground/*.json

# Include documentation
include README.md
include LICENSE

# Exclude unnecessary files
global-exclude __pycache__
global-exclude *.py[cod]
global-exclude .DS_Store
global-exclude *.so
